<body id="top">

    <!-- header
    ================================================== -->
    <header class="s-header">

        <div class="header-logo">
            <a class="site-logo" href="index.html">
                <img src="images/logotf.png" alt="Homepage">
            </a>
        </div>


    </header> <!-- end s-header -->

    <!-- search results
    ================================================== -->
    <div class="container-fluid s-search">
        <div class="intro-wrap" style="background-color: #fff;">
                 <!-- end section-header -->
                <div class="row card text-center">
                    <div class="col-sm">
                        <button  onclick="javascript:window.location.href='/'" style="line-height: normal" class="btn-block btn-primary mt-3 mb-1 next mt-4">Back</button>
                    </div>
                    <div class="col-sm">
                        
                        <!-- Multiselect dropdown -->
                        <select id="growth" multiple class="demo-default" placeholder="Select a growth form">
                            <option value="Shrub">Shrub</option>
                            <option value="Succulent">Succulent</option>
                            <option value="Grass">Grass</option>
                            <option value="Tree">Tree</option>
                        </select>
                    </div>
                    <div class="col-sm ">
                        
                        <!-- Multiselect dropdown -->
                        <select id="leafa" multiple class="demo-default" placeholder="Select arrangement">
                            <option value="Basal">Basal</option>
                            <option value="Whorled">Whorled</option>
                            <option value="Alternate">Alternate</option>
                            <option value="Opposite">Opposite</option>
                        </select>
                    </div>
                    <div class="col-sm">
                        
                        <!-- Multiselect dropdown -->
                        <select id="leaff" multiple class="demo-default" placeholder="Select leaf form">
                            <option value="Compound">Compound</option>
                            <option value="Fern-like">Fern-like</option>
                            <option value="Needle">Needle</option>
                            <option value="Simple">Simple</option>
                        </select>
                    </div>
                    <div class="col-sm">
                        
                        <!-- Multiselect dropdown -->
                        <button onclick="javascript:window.location.href='/db2?c1='+$('#growth').val()[0]+'&c2='+$('#growth').val()[1]+'&c3='+$('#growth').val()[2]+'&c4='+$('#growth').val()[3]+'&d1='+$('#leafa').val()[0]+'&d2='+$('#leafa').val()[1]+'&d3='+$('#leafa').val()[2]+'&d4='+$('#leafa').val()[3]+'&e1='+$('#leaff').val()[0]+'&e2='+$('#leaff').val()[1]+'&e3='+$('#leaff').val()[2]+'&e4='+$('#leaff').val()[3]" style="line-height: normal" class="btn-block btn-primary mt-3 mb-1 next mt-4">SEARCH</button>
                    </div>
                </div>
            <div class="row section-header">
                    <div class="col-full">
                    <h2>Search Results</h2>
                    </div>
                </div>
            <div class="container">
                <table class="table table-bordered">
  <thead>
    <tr>
      <th scope="col">Common Name</th>
      <th scope="col">Scientific Name</th>
      <th scope="col">Image</th>
      <th scope="col">Control Measures</th>
    </tr>
  </thead>
  <tbody>
      <% results.forEach(function(r) { %>
    <tr>
      <th scope="row"><%= r.common_name %></th>
      <td><%= r.scientific_name %></td>
      <td><img class="icon icon1 fit-image" src="images/weeds/<%= r.common_name %>.jpg"></td>
      <td><%= r.control_measures %></td>
    </tr>
<% }); %>
      <tr>
      <th scope="row">No more</th>
      <td>No more</td>
      <td>No more</td>
      <td>No more</td>
    </tr>
  </tbody>
</table>
            </div>
            <!-- end intro-wrap --> 
        </div>
    </div>
    <!--Reference to jQuery-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
    <!--select2 JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.6-rc.0/js/select2.min.js"></script>
    <!--selectize JS-->
    <script src="/js/selectize.js"></script>
    <script>
        $(document).ready(function () {
            $("#reset").click(function (e) {
                location.reload();
            });

            //selectize code
            var state2 = $("#growth").selectize();
            $("#div2 #fetchValue").click(function (e) {
                alert($("#growth").val());
            });
            $("#div2 #setValue").click(function (e) {
                var defValue = ["FL", "UT"];
                var selectize = state2[0].selectize;
                selectize.setValue(defValue);
            });

            //selectize code
            var state2 = $("#leafa").selectize();
            $("#div2 #fetchValue").click(function (e) {
                alert($("#leafa").val());
            });
            $("#div2 #setValue").click(function (e) {
                var defValue = ["FL", "UT"];
                var selectize = state2[0].selectize;
                selectize.setValue(defValue);
            });

            //selectize code
            var state2 = $("#leaff").selectize();
            $("#div2 #fetchValue").click(function (e) {
                alert($("#leaff").val());
            });
            $("#div2 #setValue").click(function (e) {
                var defValue = ["FL", "UT"];
                var selectize = state2[0].selectize;
                selectize.setValue(defValue);
            });
        });
    </script>
    <!-- end results -->


